{
    "name": "aero-notify",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "tsx server.ts",
        "dev:debug": "tsx --inspect server.ts",
        "build": "next build",
        "start": "npm run build && NODE_ENV=production node --import tsx server.ts",
        "lint": "eslint",
        "db:up": "docker compose up aero-notify-db -d",
        "db:down": "docker compose down aero-notify-db",
        "db:restart": "npm run db:down && npm run db:up"
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "dotenv": "^17.2.3",
        "jsonwebtoken": "^9.0.3",
        "next": "16.1.1",
        "node-cron": "^4.2.1",
        "nodemailer": "^7.0.12",
        "pg": "^8.16.3",
        "react": "19.2.3",
        "react-dom": "19.2.3"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4",
        "@types/bcrypt": "^6.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.19.27",
        "@types/node-cron": "^3.0.11",
        "@types/nodemailer": "^7.0.4",
        "@types/pg": "^8.16.0",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "babel-plugin-react-compiler": "1.0.0",
        "eslint": "^9",
        "eslint-config-next": "16.1.1",
        "tailwindcss": "^4",
        "tsx": "^4.21.0",
        "typescript": "^5"
    }
}
